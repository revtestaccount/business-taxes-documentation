<xsd:schema targetNamespace="http://www.ros.ie/schemas/form11/v5/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.ros.ie/schemas/form11/v5/" xmlns:ros="http://www.ros.ie/schemas/rostypes/" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:import namespace="http://www.ros.ie/schemas/rostypes/" schemaLocation="../../rostypes/schema.xsd"/>
	<xsd:element name="Form11">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Personal"/>
				<xsd:element ref="Trade" minOccurs="0" maxOccurs="3"/>
				<xsd:element ref="Rental" minOccurs="0"/>
				<xsd:element ref="Invest" minOccurs="0"/>
				<xsd:element ref="Paye" minOccurs="0"/>
				<xsd:element ref="OtherIrish" minOccurs="0"/>
				<xsd:element ref="Uk" minOccurs="0"/>
				<xsd:element ref="Foreign" minOccurs="0"/>
				<xsd:element ref="Exempt" minOccurs="0"/>
				<xsd:element ref="Allowances" minOccurs="0"/>
				<xsd:element ref="AllowancesInterim" minOccurs="0"/>
				<xsd:element ref="CapitalGains" minOccurs="0"/>
				<xsd:element ref="ChargesDeductions" minOccurs="0"/>
				<xsd:element ref="SummaryCalculation" minOccurs="0"/>
				<xsd:element ref="TxLineItems" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="periodstart" type="ros:date" use="required"/>
			<xsd:attribute name="periodend" type="ros:date" use="required"/>
			<xsd:attribute name="currency" type="ros:currency" use="required" fixed="E"/>
			<xsd:attribute name="formversion" type="xsd:integer" use="required" fixed="5"/>
			<xsd:attribute name="language" type="ros:language" use="required"/>
			<xsd:attribute name="shortnoarequested" type="ros:boolean" default="false"/>
			<xsd:attribute name="product" type="ros:alpha20" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Personal">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Details">
					<xsd:complexType>
						<xsd:attribute name="surname" type="ros:alpha20" use="required"/>
						<xsd:attribute name="firstname" type="ros:alpha20" use="required"/>
						<xsd:attribute name="dobself" type="ros:date" use="optional"/>
						<xsd:attribute name="ppsnself" type="ros:registration" use="required"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Marital">
					<xsd:complexType>
						<xsd:attribute name="status" use="required">
							<xsd:simpleType>
								<xsd:restriction base="xsd:integer">
									<xsd:enumeration value="0"/>
									<xsd:enumeration value="1"/>
									<xsd:enumeration value="2"/>
									<xsd:enumeration value="3"/>
									<xsd:enumeration value="4"/>
									<xsd:enumeration value="5"/>
									<xsd:enumeration value="6"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="stanceschange" type="ros:boolean" default="false"/>
						<xsd:attribute name="newdate" type="ros:date" use="optional"/>
						<xsd:attribute name="prevstatus" default="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:integer">
									<xsd:enumeration value="0"/>
									<xsd:enumeration value="1"/>
									<xsd:enumeration value="2"/>
									<xsd:enumeration value="3"/>
									<xsd:enumeration value="4"/>
									<xsd:enumeration value="5"/>
									<xsd:enumeration value="6"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="assessment" default="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:integer">
									<xsd:enumeration value="0"/>
									<xsd:enumeration value="1"/>
									<xsd:enumeration value="2"/>
									<xsd:enumeration value="3"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="ppsnspouse" type="ros:registration" use="optional"/>
						<xsd:attribute name="dobspouse" type="ros:date" use="optional"/>
						<xsd:attribute name="dateofdeath" type="ros:date" use="optional"/>
						<xsd:attribute name="areassessspouse" type="ros:boolean" default="false"/>
						<xsd:attribute name="wereassessspouse" type="ros:boolean" default="false"/>
						<xsd:attribute name="aremaintspouse" type="ros:boolean" default="false"/>
						<xsd:attribute name="weremaintspouse" type="ros:boolean" default="false"/>
						<xsd:attribute name="previousassessment" default="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:integer">
									<xsd:enumeration value="0"/>
									<xsd:enumeration value="1"/>
									<xsd:enumeration value="2"/>
									<xsd:enumeration value="3"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:attribute>
						<xsd:attribute name="nochildren" type="xsd:integer" use="optional"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Indicators" minOccurs="0">
					<xsd:complexType>
						<xsd:attribute name="medcardself" type="ros:boolean" default="false"/>
						<xsd:attribute name="medcardspouse" type="ros:boolean" default="false"/>
						<xsd:attribute name="prsiself" type="ros:boolean" default="false"/>
						<xsd:attribute name="prsispouse" type="ros:boolean" default="false"/>
						<xsd:attribute name="dirself" type="ros:boolean" default="false"/>
						<xsd:attribute name="dirspouse" type="ros:boolean" default="false"/>
						<xsd:attribute name="domicileself" type="ros:boolean" default="false"/>
						<xsd:attribute name="domicilespouse" type="ros:boolean" default="false"/>
						<xsd:attribute name="citizenself" type="ros:boolean" default="false"/>
						<xsd:attribute name="citizenspouse" type="ros:boolean" default="false"/>
						<xsd:attribute name="farmerself" type="ros:boolean" default="false"/>
						<xsd:attribute name="farmerspouse" type="ros:boolean" default="false"/>
						<xsd:attribute name="expressdoubt" type="ros:boolean" default="false"/>
						<xsd:attribute name="details" type="ros:alpha1000" use="optional"/>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Trade">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="TradeInfo">
					<xsd:complexType>
						<xsd:attribute name="tradedes" type="ros:alpha40" use="required"/>
						<xsd:attribute name="incomeforspouse" type="ros:boolean" default="false"/>
						<xsd:attribute name="cease" type="ros:date" use="optional"/>
						<xsd:attribute name="profitdevelop" type="ros:positive8" use="optional"/>
						<xsd:attribute name="profityear" type="ros:positive8" use="required"/>
						<xsd:attribute name="balcharges" type="ros:positive8" use="optional"/>
						<xsd:attribute name="unusedcap" type="ros:positive8" use="optional"/>
						<xsd:attribute name="moretradno" type="ros:positive2" use="optional"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="TradeCapital" minOccurs="0">
					<xsd:complexType>
						<xsd:attribute name="machinery" type="ros:positive8" use="optional"/>
						<xsd:attribute name="industrial" type="ros:positive8" use="optional"/>
						<xsd:attribute name="other" type="ros:positive8" use="optional"/>
						<xsd:attribute name="addunused" type="ros:boolean" default="false"/>
						<xsd:attribute name="expplantmachinery" type="ros:boolean" default="false"/>
						<xsd:attribute name="propertyincentive" type="ros:boolean" default="false"/>
						<xsd:attribute name="urbanrenewalowner" type="ros:positive8" use="optional"/>
						<xsd:attribute name="urbanrenewalinvestor" type="ros:positive8" use="optional"/>
						<xsd:attribute name="townrenewalowner" type="ros:positive8" use="optional"/>
						<xsd:attribute name="townrenewalinvestor" type="ros:positive8" use="optional"/>
						<xsd:attribute name="seasideowner" type="ros:positive8" use="optional"/>
						<xsd:attribute name="seasideinvestor" type="ros:positive8" use="optional"/>
						<xsd:attribute name="ruralrenewalowner" type="ros:positive8" use="optional"/>
						<xsd:attribute name="ruralrenewalinvestor" type="ros:positive8" use="optional"/>
						<xsd:attribute name="multistoreycarowner" type="ros:positive8" use="optional"/>
						<xsd:attribute name="multistoreycarinvestor" type="ros:positive8" use="optional"/>
						<xsd:attribute name="livingovershopowner" type="ros:positive8" use="optional"/>
						<xsd:attribute name="livingovershopinvestor" type="ros:positive8" use="optional"/>
						<xsd:attribute name="enterpriseareaowner" type="ros:positive8" use="optional"/>
						<xsd:attribute name="enterpriseareainvestor" type="ros:positive8" use="optional"/>
						<xsd:attribute name="parkrideowner" type="ros:positive8" use="optional"/>
						<xsd:attribute name="parkrideinvestor" type="ros:positive8" use="optional"/>
						<xsd:attribute name="hotelowner" type="ros:positive8" use="optional"/>
						<xsd:attribute name="hotelinvestor" type="ros:positive8" use="optional"/>
						<xsd:attribute name="holidaycottageowner" type="ros:positive8" use="optional"/>
						<xsd:attribute name="holidaycottageinvestor" type="ros:positive8" use="optional"/>
						<xsd:attribute name="nursinghomeowner" type="ros:positive8" use="optional"/>
						<xsd:attribute name="nursinghomeinvestor" type="ros:positive8" use="optional"/>
						<xsd:attribute name="housingelderlyowner" type="ros:positive8" use="optional"/>
						<xsd:attribute name="housingelderlyinvestor" type="ros:positive8" use="optional"/>
						<xsd:attribute name="convalescenthomeowner" type="ros:positive8" use="optional"/>
						<xsd:attribute name="convalescenthomeinvestor" type="ros:positive8" use="optional"/>
						<xsd:attribute name="privatehospitalowner" type="ros:positive8" use="optional"/>
						<xsd:attribute name="privatehospitalinvestor" type="ros:positive8" use="optional"/>
						<xsd:attribute name="sportsinjuryclinicowner" type="ros:positive8" use="optional"/>
						<xsd:attribute name="sportsinjuryclinicinvestor" type="ros:positive8" use="optional"/>
						<xsd:attribute name="childcarebuildingowner" type="ros:positive8" use="optional"/>
						<xsd:attribute name="childcarebuildinginvestor" type="ros:positive8" use="optional"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Losses" minOccurs="0">
					<xsd:complexType>
						<xsd:attribute name="priorloss" type="ros:positive8" use="optional"/>
						<xsd:attribute name="currentloss" type="ros:positive8" use="optional"/>
						<xsd:attribute name="setloss" type="ros:boolean" default="false"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="PSWT" minOccurs="0">
					<xsd:complexType>
						<xsd:attribute name="pswt" type="xsd:decimal" use="optional"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Review" minOccurs="0">
					<xsd:complexType>
						<xsd:attribute name="havechangedperiod" type="ros:boolean" default="false"/>
						<xsd:attribute name="adjustedprofits" type="ros:positive8" use="optional"/>
						<xsd:attribute name="profitsassessed" type="ros:positive8" use="optional"/>
						<xsd:attribute name="additionalincome" type="ros:positive8" use="optional"/>
						<xsd:attribute name="additionalincometax" type="xsd:decimal" use="optional"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="AccountChoices">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="AccountChoice">
								<xsd:complexType>
									<xsd:attribute name="accounts" use="required">
										<xsd:simpleType>
											<xsd:restriction base="xsd:integer">
												<xsd:enumeration value="0"/>
												<xsd:enumeration value="1"/>
												<xsd:enumeration value="2"/>
												<xsd:enumeration value="3"/>
												<xsd:enumeration value="4"/>
												<xsd:enumeration value="5"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
									<xsd:attribute name="partnershipppsn" type="ros:registration" use="optional"/>
