eview the Notes documentation for key/value pairs.============================================================================ --><xsd:schema targetNamespace="http://www.ros.ie/schemas/form1/v1/" xmlns:ros="http://www.ros.ie/schemas/rostypes/" xmlns="http://www.ros.ie/schemas/form1/v1/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">	<xsd:import namespace="http://www.ros.ie/schemas/rostypes/" schemaLocation="../../rostypes/schema.xsd"/>	<xsd:element name="Form1">		<xsd:complexType>			<xsd:sequence>				<xsd:element ref="Partnership"/>				<xsd:element ref="Trade" maxOccurs="2"/>				<xsd:element ref="Exempt" minOccurs="0"/>				<xsd:element ref="Rental" minOccurs="0"/>				<xsd:element ref="OtherIrish" minOccurs="0"/>				<xsd:element ref="ForeignIncome" minOccurs="0"/>				<xsd:element ref="OtherIncome" minOccurs="0"/>				<xsd:element ref="ChargesDeductions" minOccurs="0"/>				<xsd:element ref="CapitalGains" minOccurs="0"/>			</xsd:sequence>			<xsd:attribute name="periodstart" type="ros:date" use="required"/>			<xsd:attribute name="periodend" type="ros:date" use="required"/>			<xsd:attribute name="currency" type="ros:currency" use="required" fixed="E"/>			<xsd:attribute name="formversion" type="xsd:integer" use="required" fixed="6"/>			<xsd:attribute name="language" type="ros:language" use="required"/>			<xsd:attribute name="product" type="ros:alpha20" use="optional"/>		</xsd:complexType>	</xsd:element>	<xsd:element name="Partnership">		<xsd:complexType>			<xsd:sequence>				<xsd:element name="Details">					<xsd:complexType>						<xsd:attribute name="tain" type="ros:alpha6" use="optional"/>						<xsd:attribute name="agentref" type="ros:alpha30" use="optional"/>						<xsd:attribute name="partnershipname" type="ros:alpha40" use="required"/>						<xsd:attribute name="partnershipnum" type="ros:registration" use="required"/>						<xsd:attribute name="dtlschngmrkr" type="ros:boole