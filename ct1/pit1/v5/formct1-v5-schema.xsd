						<xsd:attribute name="indhotelinvestor" type="ros:positive8" use="optional"/>
						<xsd:attribute name="indholidaycottageowner" type="ros:positive8" use="optional"/>
						<xsd:attribute name="indholidaycottageinvestor" type="ros:positive8" use="optional"/>
