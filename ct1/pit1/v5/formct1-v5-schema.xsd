						<xsd:attribute name="restownrenewalinvestor" type="ros:positive8" use="optional"/>
						<xsd:attribute name="resseasideinvestor" type="ros:positive8" use="optional"/>
						<xsd:attribute name="resruralrenewalinvestor" type="ros:positive8" use="optional"/>
