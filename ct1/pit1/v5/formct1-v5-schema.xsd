													<xsd:restriction base="xsd:string">
														<xsd:enumeration value="old"/>
														<xsd:enumeration value="new"/>
														<xsd:enumeration value="higher"/>
														<xsd:enumeration value="tonnage"/>
													</xsd:restriction>
												</xsd:simpleType>
											</xsd:attribute>
										</xsd:extension>
									</xsd:complexContent>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
