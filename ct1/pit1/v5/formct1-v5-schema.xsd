									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="SmallCompaniesRelief">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="TradeDeductions">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="OtherTradeDeductions">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="ReliefSurrendered">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="DoubleTaxationRelief">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="OtherReliefs">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="TaxBroughtForward">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
						<xsd:attribute name="total" type="xsd:integer" use="optional"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Surcharges">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="SurchargesSection440">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="SurchargesSection441">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="PayableUnderDeduction">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="TRS">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
						<xsd:attribute name="total" type="xsd:integer" use="optional"/>
						<xsd:attribute name="totalreliefssurcharges" type="xsd:integer" use="optional"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Credits">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="GrossWithholdingTax">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="OtherIncomeTax">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
