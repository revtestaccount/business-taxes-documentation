									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="RentalIncome">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Interest">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="ForeignDividendIncome">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
