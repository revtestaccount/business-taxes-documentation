							</xsd:element>
							<xsd:element name="ForeignLifePolicyGain">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="OffshoreFundIncome">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="OffshoreFundGain">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="OtherIncome">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="CapitalGains">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="PreviousGains">
