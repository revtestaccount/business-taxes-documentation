							</xsd:element>
						</xsd:sequence>
						<xsd:attribute name="total" type="xsd:integer" use="optional"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="TaxableIncome" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CorporationTax" minOccurs="0" maxOccurs="4">
								<xsd:complexType>
