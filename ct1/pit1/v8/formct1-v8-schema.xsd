									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="TradeIncome">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="ExceptedTradesIncome">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
									<xsd:attribute name="allowed" type="xsd:integer" use="optional"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="ShippingIncome">
								<xsd:complexType>
									<xsd:attribute name="entered" type="xsd:integer" use="optional"/>
