			<xsd:attribute name="product" type="text20" use="optional"/>
			<xsd:attribute name="numamendments" type="positive3" use="optional"/>
			<xsd:attribute name="indicaterestrictionaccepted" type="boolean" use="optional" default="false"/>
		</xsd:complexType>
	</xsd:element>
