	<xsd:simpleType name="ppsn">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{1,8}[A-Wa-w][A-ITWXZa-itwxz ]?"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="itpmaxdecimal">
		<xsd:restriction base="xsd:decimal">
			<xsd:totalDigits value="13"/>
			<xsd:fractionDigits value="2"/>
			<xsd:minInclusive value="0.00"/>
			<xsd:pattern value="[0-9]{1,13}\.[0-9]{2}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="itpsigneddecimal">
		<xsd:restriction base="xsd:decimal">
			<xsd:totalDigits value="13"/>
			<xsd:fractionDigits value="2"/>
			<xsd:minInclusive value="-9999999999.99"/>
			<xsd:maxInclusive value="9999999999.99"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="percentage3_2">
		<xsd:restriction base="xsd:decimal">
			<xsd:totalDigits value="5"/>
			<xsd:fractionDigits value="2"/>
			<xsd:minInclusive value="0.00"/>
			<xsd:maxInclusive value="100.00"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="typeofinterest">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="e"/>
			<xsd:enumeration value="o"/>
			<xsd:enumeration value="c"/>
			<xsd:enumeration value="g"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="typeofarrangement">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="e"/>
			<xsd:enumeration value="y"/>
			<xsd:enumeration value="n"/>
			<xsd:enumeration value="na"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="AcknowledgementType">
		<xsd:choice minOccurs="0">
			<xsd:element ref="ErrorList"/>
			<xsd:element name="NoticeNumber">
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="xsd:string"/>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
		</xsd:choice>
		<xsd:attribute name="success" type="boolean" use="required"/>
	</xsd:complexType>
	<xsd:element name="ErrorList">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Error" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="ErrorList" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
			<xsd:attribute name="target" type="xsd:string" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Error">
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:extension base="xsd:string">
					<xsd:attribute name="target" type="xsd:string" use="required"/>
					<xsd:attribute name="code" type="xsd:integer" use="required"/>
				</xsd:extension>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
