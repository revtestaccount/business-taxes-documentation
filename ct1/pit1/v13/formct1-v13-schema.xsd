						<xsd:attribute name="name" type="text40" use="optional"/>
						<xsd:attribute name="address" type="text140" use="optional"/>
						<xsd:attribute name="taxreferenceno" type="ppsn" use="optional"/>
					</xsd:complexType>
				</xsd:element>
