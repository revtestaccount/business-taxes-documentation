			<xsd:attribute name="additionalcommentsdetails" type="text500" use="optional"/>
     		<xsd:attribute name="countryofresidencedetails" type="text35" use="optional"/>
    		<xsd:attribute name="taxIdentificationNumber" type="text35" use="optional"/>
    		<xsd:attribute name="addressofresidencedetails" type="text500" use="optional"/>
   		    <xsd:attribute name="indicatememberstateoftheeuropeancommunities" type="boolean" use="optional" default="false"/>
   		    <xsd:attribute name="amountofvaluebaseloss" type="euros12" use="optional"/>
			<xsd:attribute name="indicateamendselfassessment" type="boolean" use="optional" default="false"/>
		</xsd:complexType>
	</xsd:element>
