						<xsd:attribute name="natureofrelief" type="text40" use="optional"/>
						<xsd:attribute name="amountclaimed" type="euros12" use="optional"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="DirectorEmolument" minOccurs="0" maxOccurs="100">
