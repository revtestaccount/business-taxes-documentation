						<xsd:attribute name="valueofassetsprovidedfordirector" type="euros12" use="optional"/>
						<xsd:attribute name="valueofotherbenefits" type="euros12" use="optional"/>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="DirectorAccount" minOccurs="0" maxOccurs="100">
					<xsd:complexType>
						<xsd:attribute name="name" type="text40" use="optional"/>
						<xsd:attribute name="ppsnumber" type="ppsn" use="optional"/>
