<xsd:schema xmlns="http://www.ros.ie/schemas/formct1/v13/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.ros.ie/schemas/formct1/v13/" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:element name="FormCt1">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="CompanyDetails" minOccurs="0"/>
				<xsd:element ref="TradingResults" minOccurs="0"/>
				<xsd:element ref="RentalIncome" minOccurs="0"/>
				<xsd:element ref="IrishInvestmentIncome" minOccurs="0"/>
				<xsd:element ref="ForeignIncome" minOccurs="0"/>
				<xsd:element ref="ExemptIncome" minOccurs="0"/>
				<xsd:element ref="CapitalGains" minOccurs="0"/>
				<xsd:element ref="ChargeableAssets" minOccurs="0"/>
				<xsd:element ref="Deductions" minOccurs="0"/>
				<xsd:element ref="ResearchAndDevelopmentCredit" minOccurs="0"/>
				<xsd:element ref="FilmCorporationTaxCredit" minOccurs="0"/>
				<xsd:element ref="CapitalGainsDevelopmentLand" minOccurs="0"/>
				<xsd:element ref="ProfitsGainsRezonedLand" minOccurs="0"/>
				<xsd:element ref="CloseCompanySurcharge" minOccurs="0"/>
				<xsd:element ref="RecoveryOfIncomeTax" minOccurs="0"/>
				<xsd:element ref="DividendWithholdingTax" minOccurs="0"/>
				<xsd:element ref="OtherCodes" minOccurs="0"/>
				<xsd:element ref="ShortNotice" minOccurs="0"/>
				<xsd:element ref="PropertyBasedIncentives" minOccurs="0"/>
				<xsd:element ref="Notes" minOccurs="0"/>
				<xsd:element ref="SelfAssessmentCt" minOccurs="0"/>
				<xsd:element ref="SelfAssessmentCgtIt" minOccurs="0"/>
				<xsd:element ref="Prepopulation" minOccurs="0"/>
				<xsd:element ref="SummaryCalculation" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="currency" type="currency" use="required" fixed="E"/>
			<xsd:attribute name="formversion" type="xsd:integer" use="required" fixed="13"/>
			<xsd:attribute name="language" type="language" use="required"/>
			<xsd:attribute name="product" type="text20" use="optional"/>
			<xsd:attribute name="numamendments" type="positive3" use="optional"/>
			<xsd:attribute name="indicaterestrictionaccepted" type="boolean" use="optional" default="false"/>
		</xsd:complexType>
	</xsd:element>
